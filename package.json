{
  "name": "chain-reaction.common",
  "version": "1.0.0",
  "description": "A common set of backend glue code that can be applied across React applications",
  "main": "index.js",
  "engines": {
    "node": "4.3.x"
  },
  "scripts": {
    "compile": "babel -d lib/ src/ --presets es2015,react -s",
    "prepublish": "npm run compile",
    "dev": "nodemon --watch ./src --exec \"npm run compile\""
  },
  "author": "Eric Nograles",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "isomorphic-fetch": "^2.2.1",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "source-map-loader": "^0.1.5",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "nodemon": "^1.9.1"
  }
}
